<div class="card mb-3" style="max-width: 100%;">
  <div class="row no-gutters">
    <div class="col-md-4">
      <img src="${product.imageURL}"  alt="Album Image" class="card-img">
    </div>
    <div class="col-md-8">
      <div class="card-body">
        <h2 class="card-title">${product.artistName}</h2>
        <h2>${product.albumName}</h2>
        <p>${product.productType}</p>
        <p>${product.priceCurrencyFormat}</p>
        